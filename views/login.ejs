			type="text/css" />
		<link
			href="temp/css/colors/default.css"
			rel="stylesheet" />
	</head>
	<body class="h-100 bg-soft-primary">
		<section class="auth">
			<div class="container">
				<div class="pb-3 row justify-content-center">
					<div class="col-12 col-md-6 col-lg-6 col-sm-10 col-xl-6">
						<div class="card login-page roundedd border-1">
							<div class="card-body">
								<h4 class="text-center card-title text-white mb-1">Welcome Back</h4>
								<p
									class="text-center"
									style="color: white; opacity: 0.5">
									<%- include ('./partials/messages') %>
								</p>
								<form
									action="/login"
									method="POST"
									class="mt-4 login-form">
									<div class="row">
										<div class="col-lg-12">
											<div class="form-group">
												<!--<label class="text-white">Email</label>-->
												<div class="position-relative">
													<!--<i data-feather="user" class="fea icon-sm icons" style="color:rgba(255,255,255,0.3)"></i>-->
													<input
														type="email"
														class="form-control"
														name="email"
														id="email"
														placeholder="Enter Email"
														required />
												</div>
											</div>
										</div>
										<!--end col-->

										<div class="col-lg-12">
											<div class="form-group">
												<!--<label class="text-white">Password</label>-->
												<div class="position-relative">
													<!--<i data-feather="lock" class="fea icon-sm icons" style="color:rgba(255,255,255,0.3)"></i>-->
													<input
														type="password"
														class="form-control"
														name="password"
														id="password"
														placeholder="Enter Password"
														required />
												</div>
											</div>
										</div>
										<!--end col-->

										<div class="col-lg-12">
											<div class="d-flex justify-content-between">
												<div class="form-group">
													<div class="custom-control custom-checkbox">
														<input
															type="checkbox"
															class="custom-control-input"
															id="customCheck1"
															name="remember" />
														<label
															class="custom-control-label"
															for="customCheck1"
															style="
																color: rgba(255, 255, 255, 0.5);
																font-weight: 400;
															"
															>Remember me</label
														>
													</div>
												</div>
												<!-- <p class="mb-0 forgot-pass">
													<a
														href="#"
														style="
															color: rgba(255, 255, 255, 0.5);
															font-weight: 400;
														"
														>Forgot password ?</a
													>
												</p> -->
											</div>
										</div>
										<!--end col-->

										<div class="mb-0 col-lg-12">
											<button
												class="btn btn-primary btn-block pad"
												type="submit">
												Log In
											</button>
										</div>
										<!--end col-->

										<div class="mt-4 text-center col-lg-12"></div>
										<!--end col-->
										<!--end col-->

										<div class="text-center col-12">
											<p class="mt-0 mb-0">
												<a
													href="/register"
													style="color: rgba(255, 255, 255, 0.5)"
													>Create account</a
												>
											</p>
										</div>
										<!--end col-->

										<!--<div class="text-center col-12">-->
										<!--    <p class="mt-4 mb-0"><small class="mr-2 text-dark">&copy; Copyright  2025 &nbsp; Stock Invest Trades &nbsp; All Rights Reserved.</small>-->
										<!--    </p>-->
										<!--</div>-->
									</div>
									<!--end row-->
								</form>
							</div>
						</div>
						<!---->
					</div>
					<!--end col-->
				</div>
				<!--end row-->
			</div>
			<!--end container-->
		</section>
		<!--end section-->

		<script src="temp/js/jquery-3.5.1.min.js"></script>
		<script src="temp/js/bootstrap.bundle.min.js"></script>

		<!-- SLIDER -->
		<script src="temp/js/owl.carousel.min.js"></script>
		<script src="temp/js/owl.init.js"></script>
		<!-- Icons -->
		<script src="temp/js/feather.min.js"></script>
		<script src="temp/js/bundle.html"></script>

		<script src="temp/js/app.js"></script>
		<script src="temp/js/widget.js"></script>
	</body>
</html>
